{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],

    "no-unused-vars": "warn",
    "no-console": "error",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "end-of-line": "auto"
  }
}
